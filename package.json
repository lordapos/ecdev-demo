{
  "name": "ecdev-shop",
  "version": "1.0.0",
  "description": "ecdev-shop",
  "main": "index.js",
  "scripts": {
    "server": "nodemon backend/server",
    "backend": "node backend/server",
    "client": "npm start --prefix frontend",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm run-script build --prefix frontend",
    "start": "npm install --prefix frontend && npm run build && npm run backend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lordapos/ecdev-demo.git"
  },
